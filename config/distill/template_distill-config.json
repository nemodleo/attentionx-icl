{   "experiment_name":"<dataset>-<retriever>-<student>",

    "batch_size": 1,
    "retriever": "<random/topk>",
    "retriever_base": "all-mpnet-base-v2",
    "student": "distilgpt2",
    "shots": [1, 4, 8, 16, 32],

    "train_path":"",
    "val_path": null,
    "test_path":"",
    "task_description": "Task Instruction: Sentiment of the sentence\nLabel Space: {great, good, okay, bad, terrible}",

    "data_columns": {
    "input_columns": ["text"],
    "output_columns": ["label"]},

    "ice_dict":{
    "naive": "</E>Review: </text>\nSentiment: great </VP>%% good </P>%% okay </N>%% bad </NG>% terrible </VN>%",
    "sequence": "</E>Review: </text>\nSentiment: </Label1> </1>% </Label2> </2>% </Label3> </3>% </Label4> </4>% </Label5> </5>%",
    "binning": "</E>Review: </text>\nSentiment: </Label1> is very likely, </Label2> is likely, </Label3> is somewhat likely, </Label4> is unlikely, </Label5> is very unlikely"
    
    },

    "template_dict": {
        "0" : "</E>Review: </text>\nSentiment: terrible",
        "1" : "</E>Review: </text>\nSentiment: bad",
        "2" : "</E>Review: </text>\nSentiment: okay" ,
        "3" : "</E>Review: </text>\nSentiment: good" ,
        "4" : "</E>Review: </text>\nSentiment: great"
    },

    "label_dict": {
        "0": "terrible",
        "1": "bad",
        "2": "okay",
        "3": "good",
        "4": "great"
    },

    "column_token_map": {
        "naive":{"text": "</text>", "4" : "</VP>", "3" : "</P>", "2" : "</N>", "1" : "</NG>", "0" : "</VN>"},
        "sequence":{"text": "</text>", "0" : "</1>", "Label1" : "</Label1>", "1" : "</2>", "Label2" : "</Label2>", "2" : "</3>", "Label3" : "</Label3>", "3" : "</4>", "Label4" : "</Label4>", "4" : "</5>", "Label5" : "</Label5>" },
        "binning":{"text": "</text>", "Label1" : "</Label1>", "Label2" : "</Label2>", "Label3" : "</Label3>", "Label4" : "</Label4>", "Label5" : "</Label5>"},
        "GT": {"text": "</text>"}
    }
}